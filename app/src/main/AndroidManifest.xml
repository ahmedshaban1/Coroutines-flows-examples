<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coroutines.examples">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name=".helpers.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoroutinesFlowsExamples">
        <activity android:name=".flowactivities.SimpleFlowActivity"/>
        <activity android:name=".flowactivities.MapFlowActivity"/>
        <activity android:name=".flowactivities.ZipFlowActivity"/>
        <activity android:name=".flowactivities.TakeFlowActivity"/>
        <activity android:name=".flowactivities.CombineFlowActivity"/>
        <activity android:name=".flowactivities.FilterFlowActivity"/>



        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>